// En esta clase se definen y se controlan los atributos basicos de los tipos de usuarios que se pueden tener en la plataforma


public class Usuario {
    protected String nombre;
    protected boolean esPremin;
    
    
    
    public Usuario(String nombre, boolean esPremin)
    {
    this.nombre = nombre;
    this.esPremin = esPremin;
    }
    
    public String getNombre(){return nombre;}
    public boolean esPremin(){return esPremin;}
}
